<?xml version="1.0" encoding="utf-8"?>
<com.android.launcher3.assistant.search.SearchItemPhone
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/search_item_padding_v"
    android:paddingBottom="@dimen/search_item_padding_v"
    android:paddingStart="@dimen/search_item_padding_h"
    android:paddingEnd="@dimen/search_item_padding_h"
    android:clickable="true"
    android:background="@drawable/search_item_background"
    android:orientation="vertical"
    >

        <TextView
            android:id="@+id/item_phone_number"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="@dimen/search_item_title_text_size"
            android:textColor="@android:color/black" />

        <TextView
            android:id="@+id/item_phone_type"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:textSize="@dimen/phone_item_type_text_size"
            android:textColor="@android:color/black" />

</com.android.launcher3.assistant.search.SearchItemPhone>
